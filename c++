#include <iostream>
using namespace std;

int main() {
    // Print a welcome message
    cout << "Hello, World!" << endl;

    // Add two numbers
    int num1 = 5;
    int num2 = 10;
    int sum = num1 + num2;

    // Print the result
    cout << "The sum of " << num1 << " and " << num2 << " is " << sum << endl;

    return 0;
}
