public class Main {
    public static void main(String[] args) {
        // Print a welcome message
        System.out.println("Hello, World!");

        // Add two numbers
        int num1 = 5;
        int num2 = 10;
        int sum = num1 + num2;

        // Print the result
        System.out.println("The sum of " + num1 + " and " + num2 + " is " + sum);
    }
}
